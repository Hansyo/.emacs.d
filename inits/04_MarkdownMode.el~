(require 'markdown-mode-autoloads "~/.emacs.d/elisp/markdown-mode-2.3/markdown-mode-autoloads")
(require 'markdown-mode-pkg "~/.emacs.d/elisp/markdown-mode-2.3/markdown-mode-pkg")
(require 'markdown-mode "~/.emacs.d/elisp/markdown-mode-2.3/markdown-mode")



(use-package markdown-mode
  :ensure t
  :commands (markdown-mode gfm-mode)
  :mode (("README\\.md\\'" . gfm-mode)
         ("\\.md\\'" . markdown-mode)
         ("\\.markdown\\'" . markdown-mode))
  :init (setq markdown-command "multimarkdown"))
